				<script>
					document.addEventListener('DOMContentLoaded',function(){
						const conditionalDisplayElements = [...document.querySelectorAll('[data-conditional-on]')];
						conditionalDisplayElements.forEach( el => {
							const formEl = el.closest('form');
							const controllerFormFieldName = el.dataset['conditionalOn'];
							const controllerFormFieldElements = [...formEl.querySelectorAll(`[name="${controllerFormFieldName}"]`)];

							const onChange = (e)=>{
								const formData = new FormData(formEl);
								const currentFieldValue = formData.get(controllerFormFieldName);
								if( currentFieldValue == 1 ){ reveal(); }
								else{ hide(); }
							}
							const reveal = ()=>{el.removeAttribute('hidden')}
							const hide = ()=>{el.setAttribute('hidden','true')}
							
							controllerFormFieldElements.forEach( field => {
								hide();
								field.addEventListener('change',onChange);
							});
						})
					});
				</script>
				<!-- Address -->
				<div class="conditional-visibility" data-conditional-on="udf_659" hidden="true">
