		<script>
class VideoToggle {
    constructor(config={
        toggleID: 'bg-hero-video-toggle',
        videoID: 'bg-hero-video',
        pausedClass: 'is-paused'
    }){
        if(!document.getElementById(config.videoID)){return;}
        this.pausedClass = config.pausedClass;
        this.toggle = document.getElementById(config.toggleID);
        this.video = document.getElementById(config.videoID);

        this.main();
    }
    main = ()=>{
        this.toggle.addEventListener('click',this.togglePlay);
    }
    togglePlay = (e)=>{
        const paused = this.video.paused;
        if(paused){
            this.play();
        }else {
            this.pause();
        }
    }
    play = ()=>{
        // console.log('play video',this.video)
        this.video.play();
        this.toggle.classList.remove(this.pausedClass)
        this.toggle.setAttribute('aria-label','Pause background video');
    }
    pause = ()=>{
        // console.log('pause video',this.video)
        this.video.pause();
        this.toggle.classList.add(this.pausedClass)
        this.toggle.setAttribute('aria-label','Play background video');
    }
}
const hero = new VideoToggle();

</script>


