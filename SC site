<script>
var modalOpen = function (selector, options) {
    try {
        if (typeof $(selector).foundation == 'function') {
            $(selector).foundation('open');
        }
        return true;
    }
    catch(err) {
        console.log(err);
    }
};

var modalClose = function (selector, options) {
    try {
        if (typeof $(selector).foundation == 'function') {
           $(selector).foundation('close');
        }
        return true;
    }
    catch(err) {
        console.log(err);
    }
};

var redrawFavoriteControl = function (data, $form) {
    if (data.success_action == 'insert-favorite') {
        $form.find('button').text("Remove Favorite");
        $form.find('button').addClass('is-favorite');
        $form.find('[name=action]').attr('value', 'delete');
        // Assumes there is a trackEvent function somewhere
        trackEvent('Interact With Businesses', 'Add to Favorite', $form.data('name') + ' (' + $form.data('id') + ')');

    }
    else if(data.success_action == 'delete-favorite') {
        $form.find('button').text("Add Favorite");
        $form.find('button').removeClass('is-favorite');
        $form.find('[name=action]').attr('value', 'insert');
    }
    // Header count change
    $('[data-favorites-count]').text('' + data.success_value);
}

// Initialize tabbed map for listings
$('.tabs').on('toggled', function (event, tab) {
    googleMap.loadMap();
});

</script>

